{
  "name": "mantine-next-template",
  "version": "1.6.8",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development env-cmd -f .env next dev",
    "build:dev": "cross-env NODE_ENV=production env-cmd -f .env.development next build",
    "build:staging": "cross-env NODE_ENV=production env-cmd -f .env.rd next build",
    "build:qa": "cross-env NODE_ENV=production env-cmd -f .env.qa next build",
    "build": "cross-env NODE_ENV=production next build",
    "analyze": "ANALYZE=true next build",
    "start": "cross-env NODE_ENV=production next start",
    "typecheck": "tsc --noEmit",
    "export": "cross-env NODE_ENV=production next build && cross-env NODE_ENV=production next export",
    "lint": "next lint",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest",
    "storybook": "start-storybook -p 7001",
    "storybook:build": "build-storybook",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.11.0",
