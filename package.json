    "@mantine/dates": "6.0.2",
    "@mantine/dropzone": "^6.0.13",
    "@mantine/ds": "^6.0.11",
    "@mantine/form": "^6.0.11",
    "@mantine/hooks": "6.0.2",
    "@mantine/next": "6.0.2",
    "@mantine/notifications": "6.0.2",
    "@mantine/prism": "6.0.2",
    "@mantine/utils": "^6.0.19",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@next/bundle-analyzer": "^13.1.6",
    "@reduxjs/toolkit": "^1.9.5",
    "@tabler/icons": "^1.107.0",
    "@tabler/icons-react": "^2.4.0",
    "axios": "^1.4.0",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.6",
    "dotenv": "^16.1.4",
    "embla-carousel-autoplay": "7.0.0",
